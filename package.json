{
  "name": "with-redux",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "next-start": "next start",
    "start": "next start -p $PORT",
    "start-dev": "npm-run-all -p css-watch dev",
    "deploy": "npm run css-deploy && npm run",
    "test": "jest --watch",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass _sass/main.scss static/main.css",
    "css-postcss": "postcss --use autoprefixer --output static/main.css static/main.css",
    "css-watch": "npm run css-build -- --watch",    
    "heroku-postbuild": "npm run build",
    "lint": "eslint components features pages reducers redux",
    "lint:fix": "eslint --fix src",
    "pretty": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "bulma": "^0.7.4",
    "next": "^8.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.7",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.7.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.7.2",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.7.1",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "prettier": "1.14.3",
    "react-test-renderer": "^16.8.6"
  },
  "license": "ISC"
}
